# Enable Rewrite Engine
RewriteEngine On

# Force HTTPS
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

# Remove www (optional â€“ use only one version)
RewriteCond %{HTTP_HOST} ^www\.royalweaveclothing\.com [NC]
RewriteRule ^(.*)$ https://royalweaveclothing.com/$1 [L,R=301]

# Gzip Compression
<IfModule mod_deflate.c>
  AddOutputFilterByType DEFLATE text/
